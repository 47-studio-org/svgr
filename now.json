{
  "name": "svgr",
  "version": 2,
  "public": true,
  "alias": ["svgr.now.sh", "svgr"],
  "redirects": [
    {
      "source": "/",
      "destination": "https://react-svgr.com/playground"
    }
  ],
  "headers": [
    {
      "source": "/api/svgr",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        }
      ]
    }
  ]
}
